  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start -w",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "typeorm:r": "npm run typeorm migration:run -- -d ./src/config/database.config.ts",
    "typeorm:g": "npm run typeorm -- -d src/config/database.config.ts migration:generate ./src/migrations/%npm_config_name%",
    "typeorm:c": "npm run typeorm -- migration:create ./src/migrations/%npm_config_name%",
    "typeorm:rv": "npm run typeorm -- -d ./src/config/database.config.ts migration:revert",
    "test:large": "node --max-old-space-size=4096 ./node_modules/jest/bin/jest.js"
  },
